{
    "name": "Hiccup",
    "version": "1.0",
    "key": "",
    "description": "Keep track of time spent on the your favorite websites!",
    "permissions": [
        "https://docs.google.com/spreadsheets/",
        "https://script.google.com/",
        "https://sheets.googleapis.com/*",
        "identity",
        "identity.email",
        "tabs",
        "storage",
        "management",
        "notifications",
        "webRequestBlocking",
        "unlimitedStorage"
    ],
    "background": {
        "scripts": [
            "background.js",
            "sheets_api.js"
        ],
        "persistent": true
    },
    "browser_action": {
        "default_icon": "logo-green.png",
        "default_title": "Hiccup",
        "default_popup": "popup.html"
    },
    "oauth2": {
        "client_id": "1054040626297-ha237ab4ec7mjrg0m5bsdmqnn201o0fe.apps.googleusercontent.com",
        "spreadsheetId": "",
        "key": "",
        "scopes": [
            "https://www.googleapis.com/auth/userinfo.email",
            "profile",
            "https://www.googleapis.com/auth/spreadsheets"
        ]
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval' https://apis.google.com/; object-src 'self'",
    "manifest_version": 2,
    "web_accessible_resources": [
        "*.*"
    ]
}